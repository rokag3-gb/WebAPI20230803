{
    "version": "0.2.0",
    "configurations": [
        {
            // IntelliSense를 사용하여 C# 디버깅을 위해 존재하는 특성 찾기
            // 기존 속성 설명에 마우스 오버 사용
            // 자세한 내용은 https://github.com/dotnet/vscode-csharp/blob/main/debugger-launchjson.md을(를) 방문하세요.
            "name": ".NET Core Launch (web)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            // 대상 프레임워크를 변경한 경우 프로그램 경로를 업데이트해야 합니다.
            "program": "${workspaceFolder}/WebAPI20230803/bin/Debug/net7.0/WebAPI20230803.dll",
            "args": [],
            "cwd": "${workspaceFolder}/WebAPI20230803",
            "stopAtEntry": false,
            // ASP.NET Core가 시작될 때 웹 브라우저 실행을 활성화합니다. 자세한 내용: https://aka.ms/VSCode-CS-LaunchJson-WebBrowser
            "serverReadyAction": {
                "action": "openExternally",
                "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/Views"
            }
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach"
        }
    ]
}